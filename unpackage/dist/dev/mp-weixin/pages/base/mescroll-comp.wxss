
































/*说明*/
.notice-warp{
	z-index: 9;
	position: fixed;
	top: 0px;
	left: 0;
	width: 100%;
	height: 100rpx;/*对应mescroll-body的top值*/
	font-size: 26rpx;
	padding-top: 10rpx;
	border-bottom: 1rpx solid #eee;
	text-align: center;
	background-color: #fff;
}
.notice-warp .notice{
	color:#555;
}
.notice-warp .btn-change{
	display: inline-block;
	margin-top: 28rpx;
	padding: 6rpx 16rpx;
	border: 1rpx solid #FF6990;
	border-radius: 40rpx;
	color: #FF6990;
}
.notice-warp .btn-change:active{
	opacity: .5;
}

