@charset "UTF-8";
/* 顶部 fixed定位*/
.top-warp {
  z-index: 200;
  position: fixed;
  top: 0px;
  left: 0;
  width: 100%;
  height: 88rpx;
  padding-top: 10rpx;
  font-size: 28rpx;
  text-align: center;
  background-color: #CFE0DA;
}
/* 左边 fixed定位*/
.left-warp {
  z-index: 100;
  position: fixed;
  top: 0px;
  left: 0;
  bottom: 100rpx;
  width: 180rpx;
  padding-top: 88rpx;
  background-color: #eee;
}
.left-warp .tab {
  font-size: 28rpx;
  padding: 28rpx;
}
.left-warp .tab.active {
  background-color: #fff;
}
.mescroll-body, .mescroll-body {
  padding-left: 180rpx;
}
/* 底部 fixed定位*/
.bottom-warp {
  z-index: 200;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #FF6990;
}

